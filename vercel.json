{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "index.js"
      }
    ],
    "headers": [
      {
        "source": "/",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/apk"
          }
        ]
      }
    ]
  }
  